<jcr:root
  xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
  xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
  xmlns:cq="http://www.day.com/jcr/cq/1.0"
  xmlns:jcr="http://www.jcp.org/jcr/1.0"
  xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
  jcr:primaryType="nt:unstructured"
  jcr:title="Adaptive Form Checkout Cybersource"
  sling:resourceType="cq/gui/components/authoring/dialog"
  trackingFeature="core-components:adaptiveform-checkoutcybersource:v1"
  extraClientlibs="[core.forms.components.checkoutcybersource.v1.editor]">
    <content
      jcr:primaryType="nt:unstructured"
      sling:resourceType="granite/ui/components/coral/foundation/container"
      granite:class="cmp-adaptiveform-checkoutcybersource__editdialog">
        <items jcr:primaryType="nt:unstructured">
            <tabs
              jcr:primaryType="nt:unstructured"
              sling:resourceType="granite/ui/components/coral/foundation/tabs"
              maximized="{Boolean}true"
              granite:class="cmp-adaptiveform-base__editdialogbasic">
                <items jcr:primaryType="nt:unstructured">
                    <basic
                      jcr:primaryType="nt:unstructured"
                      jcr:title="Basic"
                      sling:resourceType="granite/ui/components/coral/foundation/container"
                      margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                              jcr:primaryType="nt:unstructured"
                              sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                              margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                      jcr:primaryType="nt:unstructured"
                                      sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <name
                                              jcr:primaryType="nt:unstructured"
                                              sling:resourceType="granite/ui/components/coral/foundation/include"
                                              path="core/fd/components/form/base/v1/base/cq:dialog/content/items/tabs/items/basic/items/columns/items/column/items/name"/>
                                            <title
                                              jcr:primaryType="nt:unstructured"
                                              sling:resourceType="granite/ui/components/coral/foundation/include"
                                              path="core/fd/components/form/base/v1/base/cq:dialog/content/items/tabs/items/basic/items/columns/items/column/items/title"/>
                                            <merchantId
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Merchant ID"
                                                fieldLabel="Merchant ID"
                                                name="./merchantId"
                                                required="{Boolean}true"
                                                value=""/>
                                            <merchantKeyId
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Merchant Key ID"
                                                fieldLabel="Merchant Key ID"
                                                name="./merchantKeyId"
                                                required="{Boolean}true"
                                                value=""/>
                                            <merchantSecretKey
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/password"
                                                fieldDescription="Merchant Secret Key"
                                                fieldLabel="Merchant Secret Key"
                                                name="./merchantSecretKey"
                                                required="{Boolean}true"
                                                value=""/>
                                            <wrapData
                                              granite:class="cmp-adaptiveform-panel__wrapData"
                                              jcr:primaryType="nt:unstructured"
                                              sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                              name="./wrapData"
                                              text="Group child components' data on form submission"
                                              uncheckedValue="false"
                                              value="true"/>
                                            <wrapData-typehint
                                              jcr:primaryType="nt:unstructured"
                                              sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                              name="./wrapData@TypeHint"
                                              value="Boolean"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </basic>
                    <help
                      jcr:primaryType="nt:unstructured"
                      sling:resourceType="granite/ui/components/coral/foundation/include"
                      path="core/fd/components/form/base/v1/base/cq:dialog/content/items/tabs/items/help"/>
                    <advanced
                      jcr:primaryType="nt:unstructured"
                      sling:resourceType="granite/ui/components/coral/foundation/include"
                      path="/mnt/overlay/fd/af/authoring/dialog/customProperties/cq:dialog/advanced"/>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
